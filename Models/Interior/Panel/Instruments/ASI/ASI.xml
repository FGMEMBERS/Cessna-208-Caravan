<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>ASI.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>  0.00</ind><dep>0</dep></entry>
      <entry><ind> 21.60</ind><dep>35</dep></entry>
      <entry><ind> 32.40</ind><dep>72</dep></entry>
      <entry><ind> 43.20</ind><dep>116</dep></entry>
      <entry><ind> 54.00</ind><dep>162</dep></entry>
      <entry><ind> 64.80</ind><dep>204</dep></entry>
      <entry><ind> 75.60</ind><dep>239</dep></entry>
      <entry><ind> 86.40</ind><dep>267</dep></entry>
      <entry><ind> 97.20</ind><dep>294</dep></entry>
      <entry><ind>108.00</ind><dep>321</dep></entry>
    </interpolation>
    <axis> 
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>  
    </center>
  </animation>
 
</PropertyList>
