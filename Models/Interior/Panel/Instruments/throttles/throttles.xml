<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>throttles.ac</path>

  <animation>
    <object-name>Mixture</object-name>
    <object-name>PropPitch</object-name>
    <object-name>Throttle</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Throttle</object-name>
    <property>controls/engines/engine/throttle</property>
    <factor> 70 </factor>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>PropPitch</object-name>
    <property>controls/engines/engine/propeller-pitch</property>
    <factor>70</factor>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Mixture</object-name>
    <property>controls/engines/engine/mixture</property>
    <factor> 70 </factor>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
