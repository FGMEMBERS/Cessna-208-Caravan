<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList> 
  
  <path>yoke.ac</path> 
  
  <animation>
    <type>translate</type>
    <object-name>Yoke</object-name>
    <property>controls/flight/elevator</property>
    <factor> -0.1 </factor>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Yoke</object-name>
    <property>controls/flight/aileron</property>
    <factor> -45 </factor>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
</PropertyList>
