<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <keyboard>
    
    <key n="100">
      <name>d</name>
      <desc>Doors</desc>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </key>
    
    <key n="68">
      <name>D</name>
      <desc>Passenger door</desc>
      <binding>
        <command>nasal</command>
        <script>doors.passenger.toggle();</script>
      </binding>
    </key>
    
    <key n="108">
      <name>l</name>
      <desc>Rudders</desc>
      <binding>
        <command>nasal</command>
        <script>doors.gouvernes.toggle();</script>
      </binding>
    </key>
    
  </keyboard>
  
</PropertyList>
