<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList include="Systems/caravan-base.xml">

  <sim>
    
    <description>Cessna 208 Caravan Wheels (YASim)</description>
    <author>Emmanuel BARANGER (3D/FDM)</author>

    <flight-model archive="y">yasim</flight-model>
    <aero archive="y">Systems/FDM/caravanW-yasim</aero>
    
    <rating>
      <FDM type="int">     3 </FDM>
      <systems type="int"> 2 </systems>
      <cockpit type="int"> 3 </cockpit>
      <model type="int">   4 </model>
    </rating>

    <tags>
      <tag>transport</tag>
      <tag>cessna</tag>
      <tag>1980s</tag>
      <tag>1990s</tag>
      <tag>2000s</tag>
      <tag>2010s</tag>
      <tag>fixed-gear</tag>
      <tag>high-wing</tag>
      <tag>ifr</tag>
      <tag>turboprop</tag>
      <tag>single-engine</tag>
      <tag>variable-pitch</tag>
    </tags>
    
    <startup>
      <splash-texture>Aircraft/Cessna-208-Caravan/Splash/caravanW-splash.png</splash-texture>
    </startup>
    
    <sound>
      <path archive="y">Aircraft/Cessna-208-Caravan/Sounds/caravanW-sound.xml</path>
    </sound>
    
    <model>
      <path archive="y">Aircraft/Cessna-208-Caravan/Models/caravanW.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
      <hide-cargo type="bool">false</hide-cargo>
    </model>
    
    <menubar include="Dialogs/caravanW-menu.xml"/>
    
    <help include="Systems/Help/helpW.xml"/>
    
  </sim>

  <input include="Systems/Keyboard/caravanW-keyboard.xml"/>
  
</PropertyList>
